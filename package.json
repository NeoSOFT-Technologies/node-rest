  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^17.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5"
  }
}
